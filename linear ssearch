#include<stdio.h>
int main()
{
	int i,j,a[10],n1,n,c;
	printf("enter the no of elements:");
	scanf("%d",&n);
	printf("enter the search element:");
	scanf("%d",&n1);
	for(i=0;i<n;i++){
	scanf("%d",&a[i]);
	}
	c=0;
	for(i=0;i<n;i++){
		if(a[i]==n1){
			printf("%d is found in %d",n1,i);		
			c=1;
		}
	}
	if(c!=1){
		printf("not found");
	}
	
	
}
