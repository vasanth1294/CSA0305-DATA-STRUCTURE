#include<stdio.h>
int factorial(int n)
{
int fibonacci(int n)
{
if(n==0)
return 0;
else if(n==1)
return 1;
else
return (fibonacci(n-1)+fibonacci(n-2));
}
}
int main()
{
int n,i;
clrscr();
printf("\nenter the limit for fibonacci:");
scanf("%d",&n);
for(i=0;i<n;i++){
printf("\nfibonacci series is %d\n",fibonacci(i));
}
getch();
return 0;
}
